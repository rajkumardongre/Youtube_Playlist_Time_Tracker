{
    "name" : "Youtube Playlist Time Tracker",
    "version" : "0.0.1",
    "manifest_version" : 2,
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":["activeTab"],
    "content_scripts": [
        {
          "matches": ["https://www.youtube.com/*"],
          "js": ["./src/script.js"]
        }
    ]
}